<h1>Listing societies</h1>

<table>
  <thead>
    <tr>
      <th>Club name</th>
      <th>Username</th>
      <th>Password</th>
      <th>Admin name</th>
      <th>Admin phone</th>
      <th>Admin email</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @societies.each do |society| %>
      <tr>
        <td><%= society.club_name %></td>
        <td><%= society.username %></td>
        <td><%= society.password %></td>
        <td><%= society.admin_name %></td>
        <td><%= society.admin_phone %></td>
        <td><%= society.admin_email %></td>
        <td><%= society.description %></td>
        <td><%= link_to 'Show', society %></td>
        <td><%= link_to 'Edit', edit_society_path(society) %></td>
        <td><%= link_to 'Destroy', society, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

