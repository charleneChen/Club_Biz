<div class="thumbnail">
  <!--<img src="data:image/svg+xml;base64," alt="300x200" style="width:300px; height:200px;">-->

  <div class="caption">
    <h3><%= event.title %></h3>
    <p>
      <% image = event.event_images.first %>
      <%#<%= image.inspect %>
      <% if image != nil %>
        <%= image_tag image.photo.url(:medium) %>
      <% end %>
    </p>
      <p>
        Type: <%= event.event_type %>
      </p>
      <p>
        Content: <%= event.content %>
      </P>
      <p>
        Date: <%= event.date.strftime("%A, %d %B, %Y") %>
      </p>
      <p>
        Time: <%= l event.time, :format => :short %>
      </p>
      <p>
        Venue: <%= event.venue %>
      </p>
      <p>
        <!--<button class="btn btn-default">-->
          <%= link_to 'Detail', society_event_path(society, event), :class=>"btn" %>
        <!--</button>-->
      </p>
  </div>
</div>